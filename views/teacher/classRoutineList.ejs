			        <% if(class_routine!=''){ %>
					<div class="table-responsive">
						<table class="table table-bordered" id="myTable">
							<thead>
								<tr>
									<th>
										#
									</th>
									<th>
										Day
									</th>
									<th>
										Routine
									</th>
									
									
								</tr>
							</thead>
							<tbody>
							<% for(let i = 0; i < class_routine.length; i++) { %>
								<tr>
								<td><%= i+1 %></td>
								<% if(i==0){ %> 
									<td>
									<%= 'SUNDAY' %>
									</td>
								<%  } %>
								<% if(i==1){ %> 
									<td>
									<%= 'MONDAY' %>
									</td>
								<%  } %>
								<% if(i==2){ %> 
									<td>
									<%= 'TUESDAY' %>
									</td>
								<%  } %>
								<% if(i==3){ %> 
									<td>
									<%= 'WEDNESDAY' %>
									</td>
								<%  } %>
								<% if(i==4){ %> 
									<td>
									<%= 'THURSDAY' %>
									</td>
								<%  } %>
								<% if(i==5){ %> 
									<td>
									<%= 'FRIDAY' %>
									</td>
								<%  } %>
								<% if(i==6){ %> 
									<td>
									<%= 'SATURDAY' %>
									</td>
								<%  } %>
								<td>
									<table>
								<%  for(let j = 0; j < class_routine[i].length; j++) { %>
								         <tr>
								        <td><%= class_routine[i][j].class_name %></td> 
										<td><%= class_routine[i][j].section_name %></td> 
										<td><%= class_routine[i][j].subject_name %></td>
										<td>
										 

		 								<%= class_routine[i][j].time_start %>	
										<%= ':' %>
										<%= class_routine[i][j].time_start_min %>
										<%= '-' %>
										<%=  class_routine[i][j].time_end %>
										<%= ':' %>
										<%= class_routine[i][j].time_end_min %>
										 
									    </td>
										<td><%= class_routine[i][j].teacher_name %>

										</td>

								<%	 }	%>
							</table>

								</td>
								</tr>
						      				  
						   <% } %>
							</tbody>
						</table>
					</div>
					<% } %>
