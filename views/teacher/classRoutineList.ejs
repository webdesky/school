		<div class="card">
			<div class="card-body">
				<h2 class="card-title"><b><u>Class Routine List</u></b></h2>
			 
				 	<form class="forms-sample" method="post" action="/teacher/classroutineList">
			            <div class="form-group row">
			             
			              <label for="exampleInputEmail2" class="col-sm-1 col-form-label">Class Name</label>
			              <div class="col-sm-3">
			            
			               <select class="form-control" id="Teacher_class_id" name="Teacher_class_id">
			                <option value="">Select Class</option>
			                <%  class_list.forEach(function(class_list){ %>
			                <option <% if(routinedata.class_id == class_list.class_id){%>  selected <% } %> value="<%= class_list.class_id  %>"><%= class_list.class_name; %></option>
			                
			                <%   }) %>
			               </select>
			              </div>
			              <label for="exampleInputPassword2" class="col-sm-1 col-form-label">Section Name</label>
			              <div class="col-sm-3">
			              
